{
  "name": "linq-partner-js",
  "version": "0.1.2",
  "description": "TypeScript SDK for the Linq Partner API",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:types": "node ./scripts/generate-types.mjs",
    "build": "npm run generate:types && tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "openapi-typescript": "^7.6.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
